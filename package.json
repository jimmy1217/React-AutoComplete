{
  "name": "preact-autocomplete",
  "version": "1.0.19",
  "description": "react or preact autocomplte component",
  "main": "lib/index.js",
  "files": [
    "*.md",
    "lib"
  ],
  "scripts": {
    "b": "./node_modules/.bin/http-server -g -o",
    "w": "webpack-dev-server --inline --hot --config ./webpack.config.dev.js",
    "p": "webpack --profile  --progress --config ./webpack.production.config.js",
    "gh": "git subtree push --prefix public origin gh-pages",
    "build": "rm -rf ./lib && NODE_ENV=commonjs ./node_modules/.bin/babel ./src/components/ -d lib && ./node_modules/.bin/postcss src/components/*.css -o lib/style.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy1217/React-AutoComplete.git"
  },
  "keywords": [
    "autocomplte"
  ],
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jsx-loader": "^0.13.2",
    "normalize.css": "^7.0.0",
